{
  "name": "tranvantri/pdf-to-html",
  "description": "This PHP class can convert your pdf files to html using poppler-utils.",
  "keywords": [
    "tranvantri",
    "pdf-to-html"
  ],
  "license": "MIT",
  "authors": [
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "pelago/emogrifier": "^v7.2",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^v8.17.0",
    "friendsofphp/php-cs-fixer": "^3.22",
    "laravel/framework": "^10.0",
    "phpstan/phpstan": "^0.12.14"
  },
  "autoload": {
    "psr-4": {
      "TvT\\PdfToHtml\\": "src/"
    }
  }
}
